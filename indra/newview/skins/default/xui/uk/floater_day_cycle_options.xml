			</text>
			<text type="string" length="1" name="WL9amHash">
				9am
			</text>
			<text type="string" length="1" name="WL12pmHash">
				12pm
			</text>
			<text type="string" length="1" name="WL3pm">
				3pm
			</text>
			<text type="string" length="1" name="WL6pm">
				6pm
			</text>
			<text type="string" length="1" name="WL9pm">
