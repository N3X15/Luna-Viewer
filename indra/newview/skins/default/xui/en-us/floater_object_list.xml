<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater
	name="tl object list"
	title="Object List"
	can_resize="true"
	can_minimize="true"
	can_close="true"
	can_drag_on_left="false"
	rect_control="FloaterObjectListRect"
	min_width="220"
	min_height="300"
	>
	
	<scroll_list
			name="tl_object_list"
			left="10"
			right="-10"
			top="-20"
			bottom="100"
			column_padding="0"
			can_resize="true"
			follows="left|top|bottom|right"
			draw_heading="true"
			multi_select="true"
			search_column="1"
			tool_tip="Hold shift or control while clicking to select multiple objects"
			>
		<column name="object_name" label="Name" dynamicwidth="true" minimumwidth ="20"/>
		<column name="object_owner" label="Owner" width="80"/>
		<column name="last_object_owner" label="LastOwner" width="80"/>
		<column name="distance" label="Distance" width="60"/>
		<column name="activity" label="A" width="16" />
	</scroll_list>
	<check_box height="16" label="Update"
		left="10" bottom_delta="-20" name="update_enabled_cb"
		width="60" follows="bottom|left"
		hidden="false" mouse_opaque="true" font="SansSerifSmall"
		initial_value="true" enabled="true" radio_style="false"
		tool_tip="Set whether the object list should update" />
	<check_box height="16" label="Polling"
		left_delta="60" bottom_delta="0" name="fetch_owner_enabled_cb"
		width="70" follows="bottom|left"
		hidden="false" mouse_opaque="true" font="SansSerifSmall"
		initial_value="true" enabled="true" radio_style="false"
		tool_tip="Actively poll server for object information." />
	<check_box height="16" label="30"
		left_delta="70" bottom_delta="0" name="show_limited30_cb"
		width="40" follows="bottom|left"
		hidden="false" mouse_opaque="true" font="SansSerifSmall"
		initial_value="true" enabled="true" radio_style="false"
		tool_tip="Limit display distance to 30." />
	<check_box height="16" label="128"
		left_delta="40" bottom_delta="0" name="show_limited_cb"
		width="40" follows="bottom|left"
		hidden="false" mouse_opaque="true" font="SansSerifSmall"
		initial_value="true" enabled="true" radio_style="false"
		tool_tip="Limit display distance to 128." />
	<check_box height="16" label="Attachments"
		left_delta="40" bottom_delta="0" name="show_attachments_cb"
		width="80" follows="bottom|left"
		hidden="false" mouse_opaque="true" font="SansSerifSmall"
		initial_value="false" enabled="true" radio_style="false"
		tool_tip="Display attachments." />

	<check_box height="16" label="Objects"
		left="10" bottom_delta="-20" name="show_objects_cb"
		width="60" follows="bottom|left"
		hidden="false" mouse_opaque="true" font="SansSerifSmall"
		initial_value="false" enabled="true" radio_style="false"
		tool_tip="Show static objects (no other features)" />
	<check_box height="16" label="Scripted"
		left_delta="60" bottom_delta="0" name="show_scripted_cb"
		width="70" follows="bottom|left"
		hidden="false" mouse_opaque="true" font="SansSerifSmall"
		initial_value="true" enabled="true" radio_style="false"
		tool_tip="Show scripted objects." />
	<check_box height="16" label="Physical"
		left_delta="70" bottom_delta="0" name="show_physical_cb"
		width="80" follows="bottom|left"
		hidden="false" mouse_opaque="true" font="SansSerifSmall"
		initial_value="true" enabled="true" radio_style="false"
		tool_tip="Display physical objects." />
	<check_box height="16" label="Temporary"
		left_delta="80" bottom_delta="0" name="show_temporary_cb"
		width="80" follows="bottom|left"
		hidden="false" mouse_opaque="true" font="SansSerifSmall"
		initial_value="true" enabled="true" radio_style="false"
		tool_tip="Display temporary objects." />

	<!-- upper row -->
	<button
		name="profile_btn"
		label="Profile"
		tool_tip="Show owners profile."
		left="5"
		bottom_delta="-24"
		width="70"
		height="20"
		font="SansSerifSmall"
		follows="bottom|left"
		enabled ="true"
		/>
	<button
		name="edit_btn"
		label="Edit"
		tool_tip="Edit object."
		left_delta="70"
		bottom_delta="0"
		width="70"
		height="20"
		font="SansSerifSmall"
		follows="bottom|left"
		enabled="false"
		/>
	<button
		name="teleport_btn"
		label="Teleport"
		tool_tip="Teleport to object position"
		left_delta="70"
		bottom_delta="0"
		width="70"
		height="20"
		font="SansSerifSmall"
		follows="bottom|left"
		/>
	<button
		name="mark_btn"
		label="Mark"
		tool_tip="Mark this object in the list"
		left_delta="70"
		bottom_delta="0"
		width="70"
		height="20"
		font="SansSerifSmall"
		follows="bottom|left"
		/>
	<button
		name="get_key_btn"
		label="Get Key"
		tool_tip="Copies object's key to the clipboard"
		left_delta="70"
		bottom_delta="0"
		width="70"
		height="20"
		font="SansSerifSmall"
		follows="bottom|left"
		/>
	<!-- lower row-->
	<button
		name="track_btn"
		label="Track"
		tool_tip="Track this object's position"
		left="5"
		bottom_delta="-24"
		width="70"
		height="20"
		font="SansSerifSmall"
		follows="bottom|left"
		/>
	<button
		name="view_in_list_btn"
		label="View"
		tool_tip="Focus camera on selected object."
		left_delta="70"
		bottom_delta="0"
		width="70"
		height="20"
		font="SansSerifSmall"
		follows="bottom|left"
		/>
	<button
		name="prev_in_list_btn"
		label="Prev"
		tool_tip="Focus camera on previous object."
		left_delta="70"
		bottom_delta="0"
		width="70"
		height="20"
		font="SansSerifSmall"
		follows="bottom|left"
		/>
	<button
		name="next_in_list_btn"
		label="Next"
		tool_tip="Focus camera on next object."
		left_delta="70"
		bottom_delta="0"
		width="70"
		height="20"
		font="SansSerifSmall"
		follows="bottom|left"
		/>
	<button
		name="mute_btn"
		label="Mute"
		tool_tip="Mute this object."
		left_delta="70"
		bottom_delta="0"
		width="70"
		height="20"
		font="SansSerifSmall"
		follows="bottom|left"
		/>

</floater>
