<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel name="script panel">
	<text_editor type="string" length="1" name="Script Editor">
		Betöltés folyamatban...
	</text_editor>
	<button label="Elment" label_selected="Save" name="Save_btn" />
	<combo_box label="Beszúr" name="Insert..." />
	<menu_bar name="script_menu">
		<menu name="File">
			<menu_item_call label="Elment" name="Save" />
			<menu_item_call label="Összes változtatás visszaállítása" name="Revert All Changes" />
		</menu>
		<menu name="Edit">
			<menu_item_call label="Visszaállít" name="Undo" />
			<menu_item_call label="Beállít" name="Redo" />
			<menu_item_separator label="-----------" name="separator" />
			<menu_item_call label="Kivág" name="Cut" />
			<menu_item_call label="Másol" name="Copy" />
			<menu_item_call label="Beszúr" name="Paste" />
			<menu_item_separator label="-----------" name="separator2" />
			<menu_item_call label="Mindet kijelöl" name="Select All" />
			<menu_item_call label="Kijelölés megszüntetése" name="Deselect" />
			<menu_item_separator label="-----------" name="separator3" />
			<menu_item_call label="Keresés / Cserél" name="Search / Replace..." />
		</menu>
		<menu name="Help">
			<menu_item_call label="Segítség" name="Help..." />
			<menu_item_call label="LSL Wiki segítség..." name="LSL Wiki Help..." />
		</menu>
	</menu_bar>
	<text name="loading">
		Betöltés folyamatban...
	</text>
	<text name="can_not_view">
		Nincs jogosultságod a kód megtekintésére.
	</text>
	<text name="public_objects_can_not_run">
		Nyilvános tárgyak nem futtathatnak kódokat
	</text>
	<text name="script_running">
		Futtatás
	</text>
</panel>
