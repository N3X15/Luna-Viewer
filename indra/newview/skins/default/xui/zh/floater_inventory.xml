				<menu_item_call label="新建头发" name="New Hair"/>
				<menu_item_call label="新建眼睛" name="New Eyes"/>
			</menu>
		</menu>
		<menu label="排序" name="Sort">
			<menu_item_check label="按名称" name="By Name"/>
