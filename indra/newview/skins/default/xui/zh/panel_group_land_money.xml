<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="土地和 L$" name="land_money_tab">
	<string name="help_text">
		社团所拥有的土地在捐献清单中详细列出。当使用土地总量少于或者等于总捐献量时，将有警告提示。“计划”，“详细”以及“在售”栏提供社团的财政状况。
	</string>
	<button label="?" name="help_button"/>
	<string name="cant_view_group_land_text">
		你没有查看这个社团所拥有土地的权限。
	</string>
	<string name="cant_view_group_accounting_text">
		你没有查看这个社团账户信息的权限。
	</string>
	<string name="loading_txt">
		载入...
	</string>
	<text name="group_land_heading">
		社团所持有土地
	</text>
	<scroll_list name="group_parcel_list">
		<column label="地块名称" name="name"/>
		<column label="区域" name="location"/>
		<column label="类型" name="type"/>
		<column label="面积" name="area"/>
		<column label="" name="hidden"/>
	</scroll_list>
	<button label="在地图中显示" label_selected="在地图中显示" name="map_button"/>
	<text name="total_contributed_land_label">
		总贡献额:
	</text>
	<text name="total_contributed_land_value">
		[AREA] 平方米
	</text>
	<text name="total_land_in_use_label">
		使用中的土地面积:
	</text>
	<text name="total_land_in_use_value">
		[AREA] 平方米
	</text>
	<text name="land_available_label">
		可用土地:
	</text>
	<text name="land_available_value">
		[AREA] 平方米
	</text>
	<text name="your_contribution_label">
		你的贡献:
	</text>
	<string name="land_contrib_error">
		无法设置您的土地捐献.
	</string>
	<text name="your_contribution_max_value">
		平方米 (最多[AMOUNT])
	</text>
	<text name="group_over_limit_text">
		社团成员必须贡献更多的土地额度，来支持土地的使用。
	</text>
	<text name="group_money_heading">
		社团资产 L$
	</text>
	<tab_container name="group_money_tab_container">
		<panel label="计划" name="group_money_planning_tab">
			<text_editor name="group_money_planning_text">
				计算中...
			</text_editor>
		</panel>
		<panel label="细节" name="group_money_details_tab">
			<text_editor name="group_money_details_text">
				计算中...
			</text_editor>
			<button label="&lt;较早" label_selected="&lt;较早" name="earlier_details_button" tool_tip="时间返回"/>
			<button label="较近 &gt;" label_selected="较近 &gt;" name="later_details_button" tool_tip="时间前进"/>
		</panel>
		<panel label="销售" name="group_money_sales_tab">
			<text_editor name="group_money_sales_text">
				计算中...
			</text_editor>
			<button label="&lt;较早" label_selected="&lt;较早" name="earlier_sales_button" tool_tip="时间返回"/>
			<button label="较近 &gt;" label_selected="较近 &gt;" name="later_sales_button" tool_tip="时间前进"/>
		</panel>
	</tab_container>
</panel>
